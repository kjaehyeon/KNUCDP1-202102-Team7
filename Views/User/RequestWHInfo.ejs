<!DOCTYPE html>
<html dir="ltr" lang="en">

<head>
  <%- include('../head') %>
  <script src="https://cdn.rawgit.com/mgalante/jquery.redirect/master/jquery.redirect.js"></script>
</head>

<body>
  <div class="wrap">
    <header>
      <%- include('../nav_Bar') %>
      <div class="page-header page-header-small">
        <div class="page-header-image" data-parallax="true" style="background-image: url('/Image/about_WH.jpg');"></div>
      </div>
    </header>
    <section>
      <div class="container-info">
        <h2 class="mt-5 mb-3">
          <%= __('RequestWHInfo_by_req_info')%>
        </h2>
        <table class="table table-bordered">
          <tbody>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_req_id')%>
              </th>
              <td><%= ReqItems.reqID %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_req_date')%>
              </th>
              <td><%= ReqItems.reqDate %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_wh_id')%>
              </th>
              <td><%= ReqItems.warehouseID %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_price')%>
              </th>
              <td><%= ReqItems.amount %> $</td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_rent_area')%>
              </th>
              <td><%= ReqItems.area %> m<sup>2</sup></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_rent_period')%>
              </th>
              <td><%= ReqItems.startDate %> ~ <%= ReqItems.endDate %></td>
            </tr>
          </tbody>
        </table>
      </div>
      <div class="container-info">
        <h2 class="mt-5 mb-3">
          <%= __('RequestWHInfo_wh_info')%>
        </h2>
        <table class="table table-bordered">
          <tbody>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_wh_id')%>
              </th>
              <td><%= WHitems.warehouseID %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_wh_name')%>
              </th>
              <td><%= WHitems.warehouseName %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_zip')%>
              </th>
              <td><%= WHitems.zipcode %></td>
            </tr>
            <tr>
              <th rowspan="2" class="thead-dark-vertical">
                <%= __('RequestWHInfo_address')%>
              </th>
              <td><%= WHitems.address %></td>
            </tr>
            <tr>
              <td><%= WHitems.addressDetail %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_email')%>
              </th>
              <td><%= WHitems.warehouseEmail %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_tel')%>
              </th>
              <td><%= WHitems.warehouseTEL %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_reg_date')%>
              </th>
              <td><%= WHitems.enrolledDate %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_land_area')%>
              </th>
              <td><%= WHitems.landArea %> m²</td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_floor_area')%>
              </th>
              <td><%= WHitems.floorArea %> m²</td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_usable_area')%>
              </th>
              <td><%= WHitems.useableArea %> m²</td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_price_per')%>
              </th>
              <td><%= WHitems.perprice %> $</td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_info')%>
              </th>
              <td><%= WHitems.infoComment %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_etc')%>
              </th>
              <td><%= WHitems.etcComment %></td>
            </tr>
          </tbody>
        </table>
      </div>
      <% if (session.type === 'admin' || session.type === 'buyer') { %>
      <div class="container-info">
        <h2 class="mt-5 mb-3">
          <%= __('RequestWHInfo_pv_info')%>
        </h2>
        <table class="table table-bordered">
          <tbody>
            <tr>
              <th class="thead-dark-vertical">
                 <%= __('RequestWHInfo_pv_id')%>
              </th>
              <td><%= PVitems.memberID %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_pv_name')%>
              </th>
              <td><%= PVitems.name %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_national')%>
              </th>
              <td><%= PVitems.national %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_zip')%>
              </th>
              <td><%= PVitems.zipcode %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_address')%>
              </th>
              <td><%= PVitems.address %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_email')%>
              </th>
              <td><%= PVitems.email %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_tel')%>
              </th>
              <td><%= PVitems.contactNumber %></td>
            </tr>
          </tbody>
        </table>
      </div>
      <% } %>
      <% if (session.type === 'provider' || session.type === 'admin') { %>
      <div class="container-info">
        <h2 class="mt-5 mb-3">
          <%= __('RequestWHInfo_by_info')%>
        </h2>
        <table class="table table-bordered">
          <tbody>
            <tr>
              <th class="thead-dark-vertical">
                 <%= __('RequestWHInfo_by_id')%>
              </th>
              <td><%= BYitems.memberID %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_by_name')%>
              </th>
              <td><%= BYitems.name %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_national')%>
              </th>
              <td><%= BYitems.national %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_zip')%>
              </th>
              <td><%= BYitems.zipcode %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_address')%>
              </th>
              <td><%= BYitems.address %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                 <%= __('RequestWHInfo_email')%>
              </th>
              <td><%= BYitems.email %></td>
            </tr>
            <tr>
              <th class="thead-dark-vertical">
                <%= __('RequestWHInfo_tel')%>
              </th>
              <td><%= BYitems.contactNumber %></td>
            </tr>
          </tbody>
        </table>
      </div>
      <% } %>
    </section>
    <footer>
      <%- include('../footer') %>
    </footer>
  </div>
</body>

</html>
